<%= render 'shared/page_title', title: "Create New Genre" %>
<%= render 'shared/errors', obj: @genre %>


<div class="row">
<div class="well col-md-10 col-md-offset-1">

<%= form_for @genre do |f| %>

<%= f.label :Genre_Title %>
<%= f.text_field :name %>

<%= f.submit 'Create Genre', class: "btn btn-success" %>

<% end %>
</div>
</div>
